<!DOCTYPE html>
<html class="no-js">
  <head>
    <meta charset="UTF-8">
    {% block stylesheets %}
      <link href="https://fonts.googleapis.com/css?family=Krub:300,400,500,600,700|Muli:300,400,700" rel="stylesheet">
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.0.0-rc.12/css/uikit.min.css"/>
      <link rel="stylesheet" href="{{ asset('assets/app.css') }}">
      <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,900" rel="stylesheet">
      <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
      <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
    {% endblock %}
  </head>
  <body>

    <div id="main-body">
      {% include 'frontend/components/smallnav.html.twig' %}
           {% block body %}{% endblock %}
           {{ render(controller(
                 'App\\Controller\\ComponentsController::fillFooter'
             )) }}
    </div>


<div class="loader">
  <div class="dot"></div>
  <div class="dot"></div>
  <div class="dot"></div>
</div>

    {% block javascripts %}
      <script src="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.0.0-rc.12/js/uikit.min.js"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.0.0-rc.12/js/uikit-icons.min.js"></script>
      <script src="https://unpkg.com/imagesloaded@4/imagesloaded.pkgd.min.js"></script>
      <script src="{{ asset('assets/app.js') }}"></script>
      <script src="https://unpkg.com/scrollreveal@4.0.0/dist/scrollreveal.min.js"></script>
      <script>

        var slideUp = {
         distance: '5%',
         origin: 'bottom',
         delay: 0,
         interval: 80,
         opacity: 0
        };

        ScrollReveal().reveal('.reveal-animation-b', slideUp);


      </script>

    {% endblock %}
  </body>
</html>
